	public void test(){
		
		int i = 6;
		
		for (i=7;i<10;i++){
			
//			System.out.print(i+" ");
			
			if (i==7){
//				System.out.print("breaking");
				break;
				}
			
		}
		
		int a = 5;
		
		long hhh = a*5-++a;
		
		if (hhh>6)
			System.out.println(true + " ddd");
		else
			System.out.println(false + " ddd");
		
		System.out.println(hhh + " " + a);
		
		
		int xx = 2;
		int test = 1+xx++;
		
		int xx2 = 4;
//		int test2= xx2 * 4 - xx2++;
		int test2 = 1;
		int test3 = test2++;
		
		System.out.println(test3);
		System.out.println("test is: " + test + " and test2 is: " + test2);
//		int xx = 0;
		int x = 5;
		int y = 10;
		int z = ++x +1 * y--;
		
		System.out.println(x + " " + y+" "+ z);
//		xx += a;
